   "source": [
    "c_lin = np.linspace(-.4, .4, 1000)\n",
    "x1_lin = 10*c_lin\n",
    "m_list = [[] for i in range(len(c_lin))]\n",
    "M_abs = -19.3\n",
    "z_obs = 0.50\n",
    "\n",
    "for i in range(len(c_lin)):\n",
    "    m_list[i] = np.asarray([m_obs(M_abs, x1_lin[k], c_lin[i], z_obs) for k in range(len(x1_lin))])\n",
    "\n",
    "m_list = np.asarray(m_list)"
   ]
  },
