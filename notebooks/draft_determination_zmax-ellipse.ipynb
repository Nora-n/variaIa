   "source": [
    "m_max = 23.1\n",
    "x1_max = []\n",
    "\n",
    "for i in range(len(x1_lin)):\n",
    "    loc = np.where(m_list[i] < m_max)\n",
    "    if np.size(loc) > 0:\n",
    "        x1_max.append(x1_lin[np.min(loc)])\n",
    "\n",
    "x1_max = np.asarray(x1_max)\n",
    "\n",
    "c_max = c_lin[:len(x1_max)]"
   ]
